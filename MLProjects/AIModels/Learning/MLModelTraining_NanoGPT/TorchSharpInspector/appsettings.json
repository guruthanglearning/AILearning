{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TorchSharpInspector": {
    "MaxConcurrentInspections": 5,
    "DefaultTimeout": "00:10:00",
    "ReportRetentionDays": 30,
    "MaxReportSizeMB": 100,
    "EnablePerformanceBenchmarks": true,
    "EnableMemoryAnalysis": true,
    "DefaultCheckpointDirectory": "./models",
    "ReportExportDirectory": "./reports"
  },
  "HealthChecks": {
    "CheckIntervalSeconds": 30,
    "TimeoutSeconds": 10
  },
  "Security": {
    "RequireHttps": false,
    "EnableRateLimiting": false,
    "RateLimiting": {
      "PermitLimit": 1000,
      "Window": "00:01:00",
      "QueueLimit": 100
    }
  }
}